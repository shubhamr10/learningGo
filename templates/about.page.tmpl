{{template "base" .}}

{{define "content"}}
    <div class="container">
        <div class="row">
            <div class="col">
                <h1>This is the about page</h1>
                <p>This is some txt</p>
                <p>
                    This came from template: {{ index .StringMap "test" }}
                </p>
                <p>
                    {{if ne (index .StringMap "remote_ip") ""}}
                        Your remote ip address is : {{ index .StringMap "remote_ip" }}
                    {{else}}
                        I not know your ip address yet. Visit the <a href="/">Homepage</a>
                    {{end}}
                </p>
            </div>
        </div>
    </div>
{{end}}